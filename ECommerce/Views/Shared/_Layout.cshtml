    <script type="text/template" id="products-template">
        <div class="row products-container">
            {{#each products}}
            <div class="col-sm-3">
                <div class="card">
                    <div class="card-body">
                        <img class="card-img-top rounded-0" src={{url}} alt="Card image cap">
                        <p class="card-text">{{name}}</p>
                        <a href="#/product/{{id}}" class="card-link">{{id}}</a>
                        <a class="btn btn-flat red-text p-1 my-1 mr-0 mml-1 collapsed" data-toggle="collapse" href="#collapseContent" aria-expanded="false" aria-controls="collapseContent"></a>
                        <i class="fas fa-cart-plus  text-muted float-right p-1 my-1" data-toggle="tooltip" data-placement="top" title="Add to Cart">
                        </i>
                        <i class="fas fa-heart text-muted float-right p-1 my-1 mr-3" data-toggle="tooltip" data-placement="top" title="Add to Wishlist"></i>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </script>

    <script type="text/template" id="cart-template">
        <div class="container">
            <div class="card">
                <table class="table table-hover shopping-cart-wrap">
                    <thead class="text-muted">
                        <tr>
                            <th scope="col">Product</th>
                            <th scope="col" width="120">Quantity</th>
                            <th scope="col" width="120">Price</th>
                            <th scope="col" width="200" class="text-right">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each products}}
                        <tr>
                            <td>
                                <figure class="media">
                                    <div class="img-wrap"><img src="http://bootstrap-ecommerce.com/main/images/items/2.jpg" class="img-thumbnail img-sm"></div>
                                    <figcaption class="media-body">
                                        <h6 class="title text-truncate">Product name goes here </h6>
                                        <dl class="param param-inline small">
                                            <dt>Size: {{id}}</dt>
                                            <dd>XXL</dd>
                                        </dl>
                                        <dl class="param param-inline small">
                                            <dt>Color: </dt>
                                            <dd>Orange color</dd>
                                        </dl>
                                    </figcaption>
                                </figure>
                            </td>
                            <td>
                                <input type="number" value="0" min="0" max="1000" step="1" />
                            </td>
                            <td>
                                <div class="price-wrap">
                                    <var class="price">USD 145</var>
                                    <small class="text-muted">(USD5 each)</small>
                                </div>
                            </td>
                            <td class="text-right">
                                <a title="" href="" class="btn btn-outline-danger" data-toggle="tooltip" data-original-title="Remove from cart"><i class="fas fa-trash-alt"></i></a>
                            </td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
                <div class="card-footer">
                    <div class="container">
                        <div class="row justify-content-end mb-4">
                            <div class="col-md-3">
                                <b>  Total price : 50.00€</b>
                            </div>
                        </div>
                        <div class="row justify-content-end">
                            <div class="col-md-3">
                                <input type="text" class="form-control" placeholder="cupone code">
                            </div>
                            <div class="col-md-7">
                                <input type="submit" class="btn btn-success" value="Use cupone">
                            </div>
                            <div class="col-md-2">
                                <a href="" class="btn btn-primary pull-right">Checkout</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

    </script>

    <script type="text/template" id="wishlist-template">
        <div class="container">
            <div class="card">
                <table class="table table-hover shopping-cart-wrap">
                    <thead class="text-muted">
                        <tr>
                            <th scope="col">Product</th>
                            <th scope="col" width="120">Quantity</th>
                            <th scope="col" width="120">Price</th>
                            <th scope="col" width="200" class="text-right">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each products}}
                        <tr>
                            <td>
                                <figure class="media">
                                    <div class="img-wrap"><img src="http://bootstrap-ecommerce.com/main/images/items/2.jpg" class="img-thumbnail img-sm"></div>
                                    <figcaption class="media-body">
                                        <h6 class="title text-truncate">Product name goes here </h6>
                                        <dl class="param param-inline small">
                                            <dt>Size: {{id}}</dt>
                                            <dd>XXL</dd>
                                        </dl>
                                        <dl class="param param-inline small">
                                            <dt>Color: </dt>
                                            <dd>Orange color</dd>
                                        </dl>
                                    </figcaption>
                                </figure>
                            </td>
                            <td>
                                <input type="number" value="0" min="0" max="1000" step="1" />
                            </td>
                            <td>
                                <div class="price-wrap">
                                    <var class="price">USD 145</var>
                                    <small class="text-muted">(USD5 each)</small>
                                </div> <!-- price-wrap .// -->
                            </td>
                            <td class="text-right">
                                <a title="" href="" class="btn btn-outline-danger mr-2" data-toggle="tooltip" data-original-title="Delete from Wishlist"><i class="fas fa-trash-alt"></i></a>
                                <a title="" href="" class="btn btn-outline-primary" data-toggle="tooltip" data-original-title="Add to Cart"> <i class="fa fa-shopping-cart"></i></a>

                            </td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
    </script>

