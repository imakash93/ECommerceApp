                        <script type="text/template" id="products-template">
                            <div class="row products-container">
                                {{#each products}}
                                <div class="col-sm-3 mb-4">
                                    <div class="card">
                                        <div class="card-body">
                                            <img class="card-img-top rounded-0" src={{imageUrl}} alt="Card image cap">
                                            <h3 class="card-title">Product Name : {{name}}</h3>
                                            <h3 class="card-title id">ProductID : {{key}}</h3>
                                            <p class="card-text">Price : {{price}}</p>
                                            <p class="card-text">Quantity Available : {{quantity}}</p>
                                            <p class="card-text">ProductID : {{key}}</p>
                                            <a type="button" href=' {{#productRoute}}{{Key}}{{/productRoute}}' class="btn btn-primary btn-lg btn-block">Read More</a>

                                        </div>
                                    </div>
                                </div>
                                {{/each}}
                            </div>
                        </script>

                        <script type="text/template" id="cart-template">
                            <div class="container">
                                <div class="card">
                                    <table class="table table-hover shopping-cart-wrap">
                                        <thead class="text-muted">
                                            <tr>
                                                <th scope="col">Product</th>
                                                <th scope="col" width="120">Quantity</th>
                                                <th scope="col" width="120">Price</th>
                                                <th scope="col" width="200" class="text-right">Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each products}}
                                            <tr>
                                                <td>
                                                    <figure class="media">
                                                        <div class="img-wrap"><img src={{imageUrl}} class="img-thumbnail img-sm custom-img"></div>
                                                        <figcaption class="media-body">
                                                            <h6 class="title text-truncate">DeliveryTime : {{deliveryTime}} Days</h6>
                                                            <dl class="param param-inline small">
                                                                <dt>ID : {{id}}</dt>
                                                            </dl>
                                                            <dl class="param param-inline small">
                                                                <dt>Color: </dt>
                                                                <dd>Orange color</dd>
                                                            </dl>
                                                        </figcaption>
                                                    </figure>
                                                </td>
                                                <td>
                                                    {{quantity}}

                                                </td>
                                                <td>
                                                    <div class="price-wrap">
                                                        Rs :
                                                        <b class="price">{{price}}</b>
                                                        <small class="text-muted">(USD5 each)</small>
                                                    </div>
                                                </td>
                                                <td class="text-right" pid={{productId}} uid={{userId}} key={{key}}>
                                                    <a title="" class="btn btn-outline-danger remove-from-cart" data-toggle="tooltip" data-original-title="Remove from cart"><i class="fas fa-trash-alt"></i></a>
                                                </td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                    </table>
                                    <div class="card-footer">
                                        <div class="container">
                                            <div class="row justify-content-start">
                                                <div class="pl-0">
                                                    <input type="text" class="form-control" id="" placeholder="Coupon code">
                                                </div>
                                                <div class="pl-0">
                                                    <input class="btn btn-success" value="Apply Code">
                                                </div>
                                                <div class="pl-3">
                                                    <btn class="btn btn-primary pull-right place-order">Checkout</btn>
                                                </div>
                                            </div>
                                            <div class="row justify-content-end">

                                            </div>
                                        </div>
                                    </div>

                        </script>


